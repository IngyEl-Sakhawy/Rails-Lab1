<h1>All Users</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Number of Articles</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.articles.count %></td>
      </tr>
    <% end %>
  </tbody>
</table>
